{
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTION_STRINGS__DEFAULTCONNECTION}"
  },
  "JwtSettings": {
    "SecretKey": "${JWT__SECRETKEY}",
    "Issuer": "OCSP.API",
    "Audience": "OCSP.Client",
    "ExpirationInMinutes": 60
  },
 "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
   "Jwt": {
    "SecretKey": "${JWT__SECRETKEY}",
    "ExpiryInHours": 24,
    "RefreshTokenExpiryInDays": 7
  }, "Email": {
    "SmtpHost": "${EMAIL__SMTPHOST}",
    "SmtpPort": "${EMAIL__SMTPPORT}",
    "Username": "${EMAIL__USERNAME}",
    "Password": "${EMAIL__PASSWORD}",
    "FromEmail": "${EMAIL__FROMEMAIL}",
    "EnableSsl": "${EMAIL__ENABLESSL}"
  },
  "Momo": {
    "PartnerCode": "${MOMO__PARTNERCODE}",
    "AccessKey": "${MOMO__ACCESSKEY}",
    "SecretKey": "${MOMO__SECRETKEY}",
    "Endpoint": "https://test-payment.momo.vn/v2/gateway/api/create",
    "RedirectUrl": "https://your-frontend-host/dashboard/projects",
    "IpnUrl": "https://your-backend-host/api/momo/webhook"
  },
  
  


  "AllowedHosts": "*"
}